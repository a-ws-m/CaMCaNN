@misc{hensmanGaussianProcessesBig2013,
  ids = {hensmanGaussianProcessesBig2013a},
  title = {Gaussian {{Processes}} for {{Big Data}}},
  author = {Hensman, James and Fusi, Nicolo and Lawrence, Neil D.},
  year = {2013},
