@incollection{fitzgeraldMultiscaleModelingNanomaterials2015,
  title = {1 - {{Multiscale}} Modeling of Nanomaterials: Recent Developments and Future Prospects},
  shorttitle = {1 - {{Multiscale}} Modeling of Nanomaterials},
  booktitle = {Modeling, {{Characterization}}, and {{Production}} of {{Nanomaterials}}},
  author = {Fitzgerald, G. and DeJoannis, J. and Meunier, M.},
  editor = {Tewary, Vinod K. and Zhang, Yong},
  year = {2015},
  month = jan,
  series = {Woodhead {{Publishing Series}} in {{Electronic}} and {{Optical Materials}}},
  pages = {3--53},
  publisher = {{Woodhead Publishing}},
  doi = {10.1016/B978-1-78242-228-0.00001-6},
  urldate = {2023-03-16},
  abstract = {In this chapter we report on the impact of molecular modeling on nanomaterials development. We first outline the types of modeling techniques that are currently used by the broad scientific community such as those based on quantum or classical mechanics as well as more recently developed mesoscale tools. Subsequently, we describe the combination of these methods into multiscale models, including hierarchical and hybrid approaches. Various classes of nanomaterials are then considered with respect to how modeling tools have been employed to study their properties and behavior. Finally, we present three case studies for specific applications in polymer nanocomposites, nanodielectrics and lithium-ion batteries.},
